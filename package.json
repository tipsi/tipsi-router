  },
  "dependencies": {
    "path-to-regexp": "2.1.0",
    "qs": "6.5.2"
  },
  "jest": {
    "setupFiles": [
      "./__tests__/setup/enzymeConfig.js"
    ],
    "testRegex": "__tests__/.*.test.js$",
    "testPathIgnorePatterns": [
