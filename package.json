  "author": "Tipsi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tipsi/tipsi-router/issues"
  },
  "homepage": "https://github.com/tipsi/tipsi-router#readme",
  "peerDependencies": {
    "@expo/ex-navigation": "isnifer/ex-navigation#6f1c6ca",
    "history": "4.7.2",
    "react-native-navigation": "1.1.335",
    "react-router-dom": "4.2.2",
    "react-router-modal": "1.3.5",
    "react": "16.2.0",
    "react-native": "0.50.4"
  },
  "devDependencies": {
    "babel-jest": "22.4.3",
    "babel-preset-env": "1.7.0",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "eslint": "4.15.0",
    "eslint-config-tipsi": "2.0.2",
    "history": "4.7.2",
    "jest": "22.4.3",
    "pre-commit": "1.2.2",
    "react": "16.3.0",
    "react-dom": "16.3.0",
    "react-router-dom": "4.2.2",
    "react-router-modal": "1.3.5"
  },
  "eslintConfig": {
